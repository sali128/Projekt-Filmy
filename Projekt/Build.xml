<?xml version="1.0" encoding="UTF-8"?>
<project name="Projekt">



<target name="compile">
	<mkdir dir="bin"/>
	<javac srcdir="src" destdir="bin"/>
</target>
<target name="jar">
	<mkdir dir="bin/jar"/>
	<jar destfile="bin/jar/Projekt.jar" basedir="bin">
		<manifest>
			<attribute name="Main-Class" value="Main"/>
		</manifest>
	</jar>
</target >

<target name="run-jar">
	<java jar="bin/jar/Projekt.jar" fork="true"/>
</target>





</project>